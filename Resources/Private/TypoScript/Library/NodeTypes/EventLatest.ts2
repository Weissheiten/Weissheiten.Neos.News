// Demo TS2 Implementation including a template to show sorting and paging
prototype(Weissheiten.News:EventLatest)>
prototype(Weissheiten.News:EventLatest) < prototype(Weissheiten.News:NewsLatest){
    // overwrite News Collection to only show Events
    @override.newsCollection =  ${q(site).find('[instanceof Weissheiten.News:Event]').get()}
    // override the abstract configuration with the settings from Configuration/Settings.yaml
    @override.configuration = ${Configuration.setting('Weissheiten.News.eventLatest')}
}